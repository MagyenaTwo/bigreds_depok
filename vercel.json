{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "^/",
      "has": [
        {
          "type": "host",
          "value": "bigredsdepok.com"
        }
      ],
      "status": 308,
      "headers": {
        "Location": "https://www.bigredsdepok.com"
      }
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
